  "license": "ISC",
  "dependencies": {
    "bootstrap": "4.0.0-alpha.5",
    "enduro": "1.4.46",
    "enduro_wysiwyg_quill": "^1.0.2",
    "font-awesome": "^4.7.0",
    "hamburgers": "^0.9.3",
    "jquery": "^3.5.0"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "parcel-bundler": "^1.6.2"
  }
}
